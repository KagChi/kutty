{
  "name": "kutty",
  "version": "0.0.8",
  "description": "Kutty is a set of accessible and reusable components for building web applications.",
  "main": "kutty.css",
  "author": "praveenjuge",
  "license": "MIT",
  "homepage": "https://kutty.netlify.app",
  "keywords": [
    "kutty",
    "tailwind",
    "UI",
    "css",
    "components"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/praveenjuge/kutty.git"
  },
  "bugs": {
    "url": "https://github.com/praveenjuge/kutty/issues"
  },
  "files": [
    "src",
    "dist",
    "kutty.css"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development concurrently \"postcss main.css -o static/build.css --watch\" \"hugo server --disableFastRender\" \"rollup -c -w\"",
    "production": "cross-env NODE_ENV=production postcss main.css -o static/build.css"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "rollup": "^2.18.1",
    "tailwindcss": "^1.4.6"
  }
}
